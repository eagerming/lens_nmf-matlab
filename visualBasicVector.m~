function visualBasicVector(W,H)
    if size(W,1) ~= 2
        error('The illustration can be only applied on 2d data');
    end
    
    num = size(H,2);
    startPoint = zeros(2,num);
    endPoint = W * H(1,:);
    
    iter = size
    for i = 1
    quiver(startPoint(1,:), startPoint(2,:), endPoint(1,:), endPoint(2,:));
    

end